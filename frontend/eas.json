    "preview-aab": {
      "android": {
        "buildType": "app-bundle"
      },
      "developmentClient": false,
      "distribution": "internal",
      "channel": "preview",
      "credentialsSource": "local"
    }
  },
  "submit": {
    "android": {}
  }
}